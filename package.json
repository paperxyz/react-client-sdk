{
	"name": "@paperxyz/react-client-sdk",
	"version": "0.0.1-alpha.2",
	"description": "Paper.xyz React Client SDK",
	"source": "src/index.ts",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.mjs",
	"browser": "dist/index.js",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build:lib": "tsup --env.NODE_ENV production --minify --format esm,cjs",
		"build": "yarn build:lib && yarn generate-types",
		"generate-types": "tsc"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/paperxyz/paper-client-sdk.git"
	},
	"author": "Paper.xyz",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/paperxyz/paper-client-sdk/issues"
	},
	"homepage": "https://github.com/paperxyz/paper-client-sdk#readme",
	"dependencies": {
		"react": "^17.0.0"
	},
	"devDependencies": {
		"@types/react": "^17.0.0",
		"postcss": "^8.4.13",
		"tsup": "^5.12.6",
		"typescript": "^4.6.3"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"splitting": false,
		"sourcemap": true,
		"clean": true
	},
	"directories": {
		"example": "examples"
	}
}
