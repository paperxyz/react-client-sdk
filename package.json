{
  "name": "@paperxyz/react-client-sdk",
  "version": "0.0.18",
  "description": "Paper.xyz React Client SDK",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "browser": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:lib": "tsup --env.NODE_ENV production --minify --format esm,cjs --watch",
    "dev:types": "tsc -w",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o src/styles/app.css",
    "dev": "concurrently \"yarn dev:css\" \"yarn dev:lib\" \"yarn dev:types\"",
    "build": "tsup --env.NODE_ENV production --minify --format esm,cjs && yarn tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paperxyz/react-client-sdk.git"
  },
  "author": "Paper.xyz",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/paperxyz/react-client-sdk/issues"
  },
  "homepage": "https://docs.paper.xyz",
  "peerDependencies": {
    "react": "^17 || ^18"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.1.0",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.1.4",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "directories": {
    "example": "examples"
  },
  "dependencies": {
    "ethers": "^5.6.9"
  }
}
