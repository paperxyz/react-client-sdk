{
  "name": "@paperxyz/react-client-sdk",
  "version": "0.0.17",
  "description": "Paper.xyz React Client SDK",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "browser": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:lib": "tsup --env.NODE_ENV production --minify --format esm,cjs --watch",
    "build": "concurrently \"yarn build:lib\" \"yarn generate-types\"",
    "generate-types": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paperxyz/react-client-sdk.git"
  },
  "author": "Paper.xyz",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/paperxyz/react-client-sdk/issues"
  },
  "homepage": "https://docs.paper.xyz",
  "peerDependencies": {
    "react": "^17 || ^18"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.25",
    "concurrently": "^7.1.0",
    "prettier": "^2.6.2",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "directories": {
    "example": "examples"
  },
  "dependencies": {
    "styled-components": "^5.3.5"
  }
}
